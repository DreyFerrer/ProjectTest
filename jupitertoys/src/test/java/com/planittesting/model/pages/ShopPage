package com.planittesting.model.pages;

import org.openqa.selenium.WebDriver;

public class ShopPage extends BasePage<ShopPage>{
   
    public ShopPage(WebDriver driver) {
        super(driver);
    }
    public List<Product> validateProducts() {
        return driver.findElements(By.class("products ng-scope"))
    }

    public List<Product> validateProducts(Function<Product, Boolean> strategy) {
        return getProducts()
    }

    public Product validateProducts(Function<Product, Boolean> strategy) {
        return validateProducts(strategy::apply)
    }


    }
    
